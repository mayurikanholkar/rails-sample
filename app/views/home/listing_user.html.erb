<h4> Hello</h4>
<section id="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="table-responsive">
          <table class="table table-bordered">
            <thead>
              <tr>
                <th style="text-align: center; width: 170px;">Name</th>
                <th style="text-align: center; width: 170px;">Email</th>
                <th style="text-align: center; width: 170px;">Phone</th>
                <th style="text-align: center; width: 170px;">Address</th>
                <th style="text-align: center; width: 170px;">Image</th>
                <th style="text-align: center; width: 170px;">Picture</th>
              </tr>
            </thead>
            <tbody>
              <% @members.each do |user| %>
                <tr>
                  <td style="text-align: center;"><%= user.username %></td>
                  <td style="text-align: center;"><%= user.email %></td>
                  <td style="text-align: center;"><%= user.contact.mobile %></td>
                  <td style="text-align: center;"><%= user.contact.address %></td>
                  <td style="text-align: center;"><%= image_tag user.image.url, :size => "150x100" %></td>
                  <td style="text-align: center;"><%= video_tag user.picture.url.to_s, :controls => true, :size => "150x100"%></td>
                  <td><%= link_to "Edit", edit_path(user)%></td>
                  <td><%= link_to "Show", show_path(user) %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</section> 


<%= link_to "Back", root_path %>